{
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true,
        "Promise": true,
        "console": true
      },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": "error",
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-extra-parens": "all",
        "camelcase": 2
    }
}