{
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true
      },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": "error",
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ]
    }
}